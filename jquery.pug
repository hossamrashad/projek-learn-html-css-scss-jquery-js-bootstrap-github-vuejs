                  span(class='comment pl-20') // effect for the element have event
                  br
                  span(class='element pl-20')
                    span(class='token') (
                    |this
                    span(class='token') )
                  span(class='effect') .hide
                    span(class='token') (
                    span(class='token') );
                  br
                  span(class='token') )};
